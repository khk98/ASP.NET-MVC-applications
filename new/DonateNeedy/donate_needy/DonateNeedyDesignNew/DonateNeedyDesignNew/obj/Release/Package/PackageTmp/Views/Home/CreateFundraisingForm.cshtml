<div class="container">
    <h2>Modal Example</h2>
    <!-- Trigger the modal with a button -->
    <button type="button" class="btn btn-info btn-lg" style="outline:none;margin-bottom:20px;" data-toggle="modal" data-target="#myModal">Open Modal</button>

    <!-- Modal -->
    <div class="modal fade" id="myModal" role="dialog">
        <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content" style="border-radius:0px;">
                <div class="modal-header Rectangle-8-Copy row">
                    <div class="col-lg-12">
                        <img class="grey600" src="~/img/createfundraisingform/grey-600 (1).svg" />
                        <h4 class="modal-title Event-created">Event created</h4>
                    </div>

                </div>
                <div class="modal-body row user-popup-center" style="margin-bottom:0px;">
                    <div class="col-lg-12">
                        <p class="The-event-has-been-c">
                            The event has been created successfully.
                            We will notify you after completing the verification.
                        </p>

                        <p class="Do-you-want-to-creat">
                            Do you want to create one more event?
                        </p>
                        <div class="scl-icons-sr-ctnr container" style="min-height:initial;">
                            <div class="Rectangle-9 col-lg-6 col-xs-6">
                                NO
                            </div>
                            <div class="Rectangle-91 col-lg-6 col-xs-6">
                                YES
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>

</div>
